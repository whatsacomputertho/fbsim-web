{
  "name": "fbsim-web",
  "version": "1.0.0-beta.2",
  "type": "module",
  "description": "A web-based american football simulation user interface built atop the fbsim-core Rust crate",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsc",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@whatsacomputertho/fbsim-ui": "^1.0.0-beta.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "hbs": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "prettier": "^3.4.0",
    "supertest": "^7.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whatsacomputertho/fbsim-web.git"
  },
  "keywords": [
    "Football",
    "Simulation",
    "Analytics"
  ],
  "author": "whatsacomputertho",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/whatsacomputertho/fbsim-web/issues"
  },
  "homepage": "https://github.com/whatsacomputertho/fbsim-web#readme"
}
